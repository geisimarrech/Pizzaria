<html class=" dark js hashchange backgroundsize boxshadow cssgradients" lang="en"><!--<![endif]--><head>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	
	<title>Pizzaria Peperone</title>
	<meta name="description" content="">
	<meta name="author" content="Geisimar Rech">
	
	<!-- Combined stylesheets load -->

	<%= stylesheet_link_tag "style","user_sessions" %>

	
	<!-- Modernizr for support detection, all javascript libs are moved right above </body> for better performance -->
	<%= javascript_include_tag "jquery", "ga", "modernizr", "rails" %>
	
</head>

<!-- the 'special-page' class is only an identifier for scripts -->
<body class="special-page login-bg dark">
<!-- The template uses conditional comments to add wrappers div for ie8 and ie7 - just add .ie, .ie7 or .ie6 prefix to your css selectors when needed -->
<!--[if lt IE 9]><div class="ie"><![endif]-->
<!--[if lt IE 8]><div class="ie7"><![endif]-->	
	
	
	<!--[if lte IE 8]>
		<%= javascript_include_tag "standard.ie.js" %>
	<![endif]-->
	
	<%= yield %>
	<!-- example login script -->
	<script>
	
		$(document).ready(function()
		{
			$("#password-recovery a").click(function(){
				$("#password-recovery fieldset").toggleClass('collapsed');
			});
			
			$(".message").click(function(){
				$(this).fadeOut();
			});
			
			$(".no_cad").click(function(){
				$("#login-block").toggle();
				$("#cad-block").toggle();
				$(".message").html("");
				$(".message").hide();
			})
			
		});
	
	</script>
	
	

<ul id="notifications"></ul></body></html>